var power = f(a,b){
    var res = 1;
    for(1,b,1, f(){
        var res = res * a;
    })
    return res;
}

write('Power program')
write('Input number a:')
var a = num(read())
write('Input number b:')
var b = num(read())

if(and(isType(a,10),isType(b,10))){
    write('The result is ' + str(power(a,b)));
}else{
    write('Invalid input, please make sure it is number')
}

